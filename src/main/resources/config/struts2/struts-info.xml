<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
    <package name="insert" extends="my-struts" namespace="/user">
        <!-- User 列表页 -->
        <action name="insert" class="com.jasper.ssm.action.SetAction">
            <!--action 返回值为success时,则返回页面user.jsp,并解析为html展示到网页-->
            <result name="success">/WEB-INF/views/success.jsp</result>
        </action>

    </package>

    <package name="index" extends="my-struts" namespace="/user">

    <!-- User 列表页 -->
    <action name="index" class="com.jasper.ssm.action.QueryAction">
        <!--action 返回值为success时,则返回页面user.jsp,并解析为html展示到网页-->
        <result name="success">/WEB-INF/views/info.jsp</result>
    </action>

</package>


</struts>